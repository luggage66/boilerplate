{
    "type": "sqlite",
    "database": "database.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
        "out/database/entity/**/*.js"
    ],
    "migrations": [
        "out/database/migration/**/*.js"
    ],
    "subscribers": [
        "out/database/subscriber/**/*.js"
    ],
    "cli": {
        "entitiesDir": "out/database/entity",
        "migrationsDir": "out/database/migration",
        "subscribersDir": "out/database/subscriber"
   }
}